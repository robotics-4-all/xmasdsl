Range Top [1-10]
Color Red [255, 0, 0]

Group Fun {
    SetBrightness(100)
    Parallel{
        Serial{
            SetBrightness(100)
        }
        Serial{
            SetBrightness(100)
        }
        Fun
    }
}

Program {
    SetBrightness(100)
    SetPixelColor(Top, Red, 1000)
    Delay(100)
    Fun
    Parallel{
        Serial{
            SetBrightness(100)
        }
        Fun
    }
}
